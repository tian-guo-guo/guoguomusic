AAA;AACA,KAHA;AAIA,IAAA,OAJA,qBAIA;AAAA;;AACA,MAAA,QAAA,CAAA;AACA,QAAA,IAAA,EAAA,KAAA;AADA,OAAA,CAAA,CAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAJA;AAKA,KAVA;AAWA,IAAA,SAXA,qBAWA,EAXA,EAWA;AAAA;;AACA,MAAA,OAAA,CAAA;AACA,QAAA,EAAA,EAAA;AADA,OAAA,CAAA,CAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA;AACA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,GAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,GAAA;AACA,OANA;AAOA;AAnBA,GATA;AA8BA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,OAAA;AACA;AAHA,GA9BA;AAmCA,EAAA,OAnCA,qBAmCA;AACA,SAAA,OAAA;AACA;AArCA,CAAA","sourcesContent":["<template>\n  <div class=\"songs-container\" >\n    <div class=\"tab-bar\">\n      <span class=\"item\" :class=\"{ active: type == 0 }\" @click=\"type = 0\"\n        >全部</span\n      >\n      <span class=\"item\" :class=\"{ active: type == 7 }\" @click=\"type = 7\"\n        >华语</span\n      >\n      <span class=\"item\" :class=\"{ active: type == 96 }\" @click=\"type = 96\"\n        >欧美</span\n      >\n      <span class=\"item\" :class=\"{ active: type == 8 }\" @click=\"type = 8\"\n        >日本</span\n      >\n      <span class=\"item\" :class=\"{ active: type == 16 }\" @click=\"type = 16\"\n        >韩国</span\n      >\n    </div>\n    <!-- 底部的table -->\n    <table class=\"el-table playlit-table\">\n      <thead>\n        <th></th>\n        <th></th>\n        <th>音乐标题</th>\n        <th>歌手</th>\n        <th>专辑</th>\n        <th>时长</th>\n      </thead>\n      <tbody>\n        <tr\n          class=\"el-table__row\"\n          v-for=\"(item, index) in tableData\"\n          :key=\"item.id\"\n        >\n          <td>{{ index + 1 }}</td>\n          <td>\n            <div class=\"img-wrap\" @click=\"playMusic(item.id)\">\n              <img :src=\"item.album.picUrl\" alt=\"\" />\n              <span  class=\"iconfont icon-play\" ></span>\n            </div>\n          </td>\n          <td>\n            <div class=\"song-wrap\">\n              <div class=\"name-wrap\">\n                <span>{{ item.name }}</span>\n                <span v-if=\"item.mvid != 0\" class=\"iconfont icon-mv\" @click='toMV(item.mvid)'></span>\n              </div>\n              <span>{{ item.subTitle }}</span>\n            </div>\n          </td>\n          <td>{{ item.artists[0].name }}</td>\n          <td>\n            {{ item.album.name }}\n          </td>\n          <td>{{ item.duration | formatDuration }}</td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport { topSongs } from '@/api/songs';\nimport { songUrl } from '@/api/discovery';\nexport default {\n  name: 'songs',\n  data() {\n    return {\n      type: 0,\n      tableData: [],\n    \n    };\n  },\n  methods: {\n    toMV(id){\n      this.$router.push(`/mv?id=${id}`)\n    },\n    getData() {\n      topSongs({\n        type: this.type\n      }).then(res => {\n        this.tableData = res.data;\n      });\n    },\n    playMusic(id) {\n      songUrl({\n        id: id\n      }).then(res => {\n        // window.console.log(res)\n        // this.songUrl = res.data[0].url\n        this.$parent.url = res.data[0].url;\n      });\n    }\n  },\n  watch: {\n    type() {\n      this.getData();\n    }\n  },\n  created() {\n    this.getData();\n  }\n};\n</script>\n\n<style lang=\"scss\"></style>\n"],"sourceRoot":"src/views"}]}