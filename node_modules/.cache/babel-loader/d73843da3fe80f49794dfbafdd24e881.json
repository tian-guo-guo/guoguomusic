A,CAAA;AACA,QAAA,IAAA,EAAA,KAAA;AADA,OAAA,CAAA,CAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAJA;AAKA,KAVA;AAWA,IAAA,SAXA,qBAWA,EAXA,EAWA;AAAA;;AACA,MAAA,OAAA,CAAA;AACA,QAAA,EAAA,EAAA;AADA,OAAA,CAAA,CAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA;AACA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,GAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,GAAA;AACA,OANA;AAOA;AAnBA,GATA;AA8BA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,OAAA;AACA;AAHA,GA9BA;AAmCA,EAAA,OAnCA,qBAmCA;AACA,SAAA,OAAA;AACA;AArCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"songs-container\" >\r\n    <div class=\"tab-bar\">\r\n      <span class=\"item\" :class=\"{ active: type == 0 }\" @click=\"type = 0\"\r\n        >全部</span\r\n      >\r\n      <span class=\"item\" :class=\"{ active: type == 7 }\" @click=\"type = 7\"\r\n        >华语</span\r\n      >\r\n      <span class=\"item\" :class=\"{ active: type == 96 }\" @click=\"type = 96\"\r\n        >欧美</span\r\n      >\r\n      <span class=\"item\" :class=\"{ active: type == 8 }\" @click=\"type = 8\"\r\n        >日本</span\r\n      >\r\n      <span class=\"item\" :class=\"{ active: type == 16 }\" @click=\"type = 16\"\r\n        >韩国</span\r\n      >\r\n    </div>\r\n    <!-- 底部的table -->\r\n    <table class=\"el-table playlit-table\">\r\n      <thead>\r\n        <th></th>\r\n        <th></th>\r\n        <th>音乐标题</th>\r\n        <th>歌手</th>\r\n        <th>专辑</th>\r\n        <th>时长</th>\r\n      </thead>\r\n      <tbody>\r\n        <tr\r\n          class=\"el-table__row\"\r\n          v-for=\"(item, index) in tableData\"\r\n          :key=\"item.id\"\r\n        >\r\n          <td>{{ index + 1 }}</td>\r\n          <td>\r\n            <div class=\"img-wrap\" @click=\"playMusic(item.id)\">\r\n              <img :src=\"item.album.picUrl\" alt=\"\" />\r\n              <span  class=\"iconfont icon-play\" ></span>\r\n            </div>\r\n          </td>\r\n          <td>\r\n            <div class=\"song-wrap\">\r\n              <div class=\"name-wrap\">\r\n                <span>{{ item.name }}</span>\r\n                <span v-if=\"item.mvid != 0\" class=\"iconfont icon-mv\" @click='toMV(item.mvid)'></span>\r\n              </div>\r\n              <span>{{ item.subTitle }}</span>\r\n            </div>\r\n          </td>\r\n          <td>{{ item.artists[0].name }}</td>\r\n          <td>\r\n            {{ item.album.name }}\r\n          </td>\r\n          <td>{{ item.duration | formatDuration }}</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { topSongs } from '@/api/songs';\r\nimport { songUrl } from '@/api/discovery';\r\nexport default {\r\n  name: 'songs',\r\n  data() {\r\n    return {\r\n      type: 0,\r\n      tableData: [],\r\n    \r\n    };\r\n  },\r\n  methods: {\r\n    toMV(id){\r\n      this.$router.push(`/mv?id=${id}`)\r\n    },\r\n    getData() {\r\n      topSongs({\r\n        type: this.type\r\n      }).then(res => {\r\n        this.tableData = res.data;\r\n      });\r\n    },\r\n    playMusic(id) {\r\n      songUrl({\r\n        id: id\r\n      }).then(res => {\r\n        // window.console.log(res)\r\n        // this.songUrl = res.data[0].url\r\n        this.$parent.url = res.data[0].url;\r\n      });\r\n    }\r\n  },\r\n  watch: {\r\n    type() {\r\n      this.getData();\r\n    }\r\n  },\r\n  created() {\r\n    this.getData();\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\"></style>\r\n"],"sourceRoot":"src/views"}]}